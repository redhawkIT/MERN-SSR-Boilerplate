    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.10.2",
    "react-test-renderer": "^15.5.3",
    "redux-mock-store": "1.2.2",
    "sinon": "^2.1.0"
  },
  "engines": {
    "node": ">=6.x"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-transform": "^2.0.2",
